<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block titulo %}Proyecto Symfony{% endblock %}</title>
        <!-- Bootstrap core CSS -->
        <link href="{{ asset('vendor/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet">
        <!-- Custom styles for this template -->
        <link href="{{ asset('css/modern-business.css') }}" rel="stylesheet">
        {% block stylesheets %}        
        <link href="{{ asset('css/estilos.css') }}" rel="stylesheet" />
        <!-- Estilos propios -->
        <link rel="stylesheet" href="{{ asset('css/gfct.css') }}">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        
            <!-- Barra de menú principal de navegación -->
    <nav class="navbar fixed-top navbar-expand-lg navbar-dark bg-dark fixed-top">
      <div class="container">
        <a class="navbar-brand" href="#">FDN Autobuses Fuente del Norte</a>
        <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarResponsive">
          <ul class="navbar-nav ml-auto">
            <li class="nav-item">
              <a class="nav-link" href="#">Nosotros</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="#">Servicios</a>
            </li>
            
            <li class="nav-item dropdown">
              <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownBlog" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                Blog
              </a>
              <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdownBlog">
                <a class="dropdown-item" href="#">Económica</a>
                <a class="dropdown-item" href="#">Premier</a>
                <a class="dropdown-item" href="#">Clase de Oro</a>
              </div>
            </li>
            
            <li class="nav-item">
              <a class="nav-link" href="#">Buses</a>
            </li>
            
          </ul>
        </div>
      </div>
    </nav>    
            <!-- FIN de Barra de menú principal de navegación -->
            
            <div class="container">
                <div class="row">        
                    <div class="col-md-3">{% block menu %}Contenido Izda{% endblock %}</div>        {# "col-md-4" para desktops #}
                    <div class="col-md-9">{% block body %}Contenido{% endblock %}</div>            {# "col-md-8" para el contenido central #}
                </div>
            </div>
        
        
        <!-- Bootstrap core JavaScript -->
        <script src="{{ asset('vendor/jquery/jquery.min.js') }}"></script>
        <script src="{{ asset('vendor/bootstrap/js/bootstrap.bundle.min.js') }}"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
